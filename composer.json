{
    "name": "tom32i/phpillip",
    "license": "MIT",
    "type": "library",
    "description": "PHP Static Site Generator",
    "homepage": "https://github.com/Tom32i/phpillip",
    "authors": [
        {
            "name": "Thomas Jarrand",
            "email": "thomas.jarrand@gmail.com",
            "homepage": "http://thomas.jarrand.fr",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phpillip\\": "src"
        }
    },
    "require": {
        "php": ">=5.6",
        "silex/silex": "~1.3",
        "symfony/config": "~2.7",
        "symfony/console": "~2.7",
        "symfony/filesystem": "~2.7",
        "symfony/finder": "~2.7",
        "symfony/process": "^2.7",
        "symfony/serializer": "~2.7",
        "symfony/twig-bridge": "^2.7",
        "symfony/yaml": "~2.7",
        "erusev/parsedown": "^1.5"
    },
    "bin": ["src/Resources/bin/console"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
